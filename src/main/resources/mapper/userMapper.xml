<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.kevin.mem.mng.domain.mapper.UserMapper">
	
	<!-- 基础数据-->
	<resultMap id="BaseResultMap" type="com.kevin.mem.mng.domain.entity.User" >
		<id column="id" property="id"/>
		<result column="username" property="username"/>
		<result column="password" property="password"/>
		<result column="status" property="status"/>
		<result column="telephone" property="telephone"/>
		<result column="mail" property="mail"/>
		<result column="dept_id" property="deptId"/>
		<result column="remark" property="remark"/>
		<result column="is_deleted" property="isDeleted"/>
		<result column="create_by" property="createBy"/>
		<result column="update_by" property="updateBy"/>
		<result column="create_time" property="createTime"/>
		<result column="update_time" property="updateTime"/>
	</resultMap>
	
	<sql id="Base_Column_List">
			id,  username,  password,  status,  telephone,  mail,  dept_id,  remark,  is_deleted,  create_by,  update_by,  create_time,  update_time
	</sql>

	<!--新增接口开始-->
	<insert id="insert" parameterType="com.kevin.mem.mng.domain.entity.User" >
		insert into t_mem_user
		<trim prefix="(" suffix=")" suffixOverrides=",">
			<if test="username != null">
			username,
			</if>
			<if test="password != null">
			password,
			</if>
			<if test="status != null">
			status,
			</if>
			<if test="telephone != null">
			telephone,
			</if>
			<if test="mail != null">
			mail,
			</if>
			<if test="deptId != null">
			dept_id,
			</if>
			<if test="remark != null">
			remark,
			</if>
			<if test="isDeleted != null">
			is_deleted,
			</if>
			<if test="createBy != null">
			create_by,
			</if>
			<if test="updateBy != null">
			update_by,
			</if>
			<if test="createTime != null">
			create_time,
			</if>
			<if test="updateTime != null">
			update_time,
			</if>
		</trim>
		<trim prefix="values (" suffix=")" suffixOverrides=",">
			<if test="username != null">
			#{username},
			</if>
			<if test="password != null">
			#{password},
			</if>
			<if test="status != null">
			#{status},
			</if>
			<if test="telephone != null">
			#{telephone},
			</if>
			<if test="mail != null">
			#{mail},
			</if>
			<if test="deptId != null">
			#{deptId},
			</if>
			<if test="remark != null">
			#{remark},
			</if>
			<if test="isDeleted != null">
			#{isDeleted},
			</if>
			<if test="createBy != null">
			#{createBy},
			</if>
			<if test="updateBy != null">
			#{updateBy},
			</if>
			<if test="createTime != null">
			#{createTime},
			</if>
			<if test="updateTime != null">
			#{updateTime},
			</if>
		</trim>
	</insert>

	<!--更新接口开始-->
	<update id="updateById" parameterType="com.kevin.mem.mng.domain.entity.User" >
		update t_mem_user
		<set>
			<if test="username != null">
				username = #{username},
			</if>
			<if test="password != null">
				password = #{password},
			</if>
			<if test="status != null">
				status = #{status},
			</if>
			<if test="telephone != null">
				telephone = #{telephone},
			</if>
			<if test="mail != null">
				mail = #{mail},
			</if>
			<if test="deptId != null">
				dept_id = #{deptId},
			</if>
			<if test="remark != null">
				remark = #{remark},
			</if>
			<if test="isDeleted != null">
				is_deleted = #{isDeleted},
			</if>
			<if test="createBy != null">
				create_by = #{createBy},
			</if>
			<if test="updateBy != null">
				update_by = #{updateBy},
			</if>
			<if test="createTime != null">
				create_time = #{createTime},
			</if>
			<if test="updateTime != null">
				update_time = #{updateTime},
			</if>
		</set>
		<where> id = #{id} </where>
	</update>

  <!--删除接口开始-->
	<delete id="deleteById" >
		delete from t_mem_user
		<where> id = #{id} </where>
	</delete>

	<delete id="batchDelete">
		delete from t_mem_user
		<where>
			id in
			<foreach collection="idList" item="id" open="(" close=")" separator=",">
			#{id}
			</foreach>
		</where>
	</delete>

  <!--查询接口开始-->
  <select id="queryPage" resultMap="BaseResultMap" parameterType="com.kevin.mem.mng.domain.entity.User">
    select
    <include refid="Base_Column_List" />
    from t_mem_user
    <where>
		<if test="username != null">
			and username = #{username}
		</if>
		<if test="password != null">
			and password = #{password}
		</if>
		<if test="status != null">
			and status = #{status}
		</if>
		<if test="telephone != null">
			and telephone = #{telephone}
		</if>
		<if test="mail != null">
			and mail = #{mail}
		</if>
		<if test="deptId != null">
			and dept_id = #{deptId}
		</if>
		<if test="remark != null">
			and remark = #{remark}
		</if>
		<if test="isDeleted != null">
			and is_deleted = #{isDeleted}
		</if>
		<if test="createBy != null">
			and create_by = #{createBy}
		</if>
		<if test="updateBy != null">
			and update_by = #{updateBy}
		</if>
		<if test="createTime != null">
			and create_time = #{createTime}
		</if>
		<if test="updateTime != null">
			and update_time = #{updateTime}
		</if>
    </where>
  </select>

  <select id="selectById" resultMap="BaseResultMap">
    select
    <include refid="Base_Column_List" />
    from t_mem_user
    <where> id = #{id} </where>
  </select>
</mapper>

	